@{
    Layout = "~/Views/Shared/_LayoutFinlab.cshtml";
}
<div class="content-body default-height ">
    <div class="container-fluid">


        <!-- Vectormap -->
        <div class="row">
            <div class="col-xl-12">
                <div class="page-titles">
                    <nav style="--bs-breadcrumb-divider: '>';" aria-label="breadcrumb">
                        <ol class="breadcrumb">
                            <li class="breadcrumb-item"><a href="#">Plugins</a></li>
                            <li class="breadcrumb-item active" aria-current="page">Map</li>
                        </ol>
                    </nav>
                </div>
            </div>
            <div class="col-lg-6">
                <div class="card">
                    <div class="card-header">
                        <h4 class="card-title">World Map</h4>
                    </div>
                    <div class="card-body">
                        <div id="world-map"></div>
                    </div>
                </div>
            </div>
            <div class="col-lg-6">
                <div class="card">
                    <div class="card-header">
                        <h4 class="card-title">USA</h4>
                    </div>
                    <div class="card-body">
                        <div id="usa" style="height: 450px;"></div>
                    </div>
                </div>
            </div>
        </div>

    </div>
</div>

@section Finlab_style
	{
	<link href="~/Finlab/vendor/jqvmap/css/jqvmap.min.css" rel="stylesheet">
}
@section Finlab_script
	{
    <script src="~/Finlab/vendor/global/global.min.js"></script>
    <script src="~/Finlab/vendor/bootstrap-select/dist/js/bootstrap-select.min.js"></script>
    <!-- Vectormap -->
    <script src="~/Finlab/vendor/jqvmap/js/jquery.vmap.min.js"></script>
    <script src="~/Finlab/vendor/jqvmap/js/jquery.vmap.world.js"></script>
    <script src="~/Finlab/vendor/jqvmap/js/jquery.vmap.usa.js"></script>
    <!-- All init script -->
    <script src="~/Finlab/js/plugins-init/jqvmap-init.js"></script>
    <script src="~/Finlab/js/custom.min.js"></script>
    <script src="~/Finlab/js/dlabnav-init.js"></script>
    
    
}