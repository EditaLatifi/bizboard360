@{
    Layout = "~/Views/Shared/_LayoutFinlab.cshtml";
}
<div class="content-body default-height">
	<div class="container-fluid">

		<div class="row">
			<div class="col-xl-12">
				<div class="page-titles">
					<nav style="--bs-breadcrumb-divider: '>';" aria-label="breadcrumb">
						<ol class="breadcrumb">
							<li class="breadcrumb-item"><a href="#">Plugins</a></li>
							<li class="breadcrumb-item active" aria-current="page">Light Gallery</li>
						</ol>
					</nav>
				</div>
			</div>
			<div class="col-lg-12">
				<div class="card">
					<div class="card-header">
						<h4 class="card-title">Light Gallery</h4>
					</div>
					<div class="card-body pb-1">
						<div id="lightgallery" class="row">
							<a href="~/Finlab/images/big/img1.jpg" data-src="../Finlab/images/big/img1.jpg" class="lg-item col-lg-3 col-md-6 mb-4">
								<img src="~/Finlab/images/big/img1.jpg" class="rounded" alt="" style="width:100%;">
							</a>
							<a href="~/Finlab/images/big/img2.jpg" data-src="../Finlab/images/big/img2.jpg" class=" lg-item col-lg-3 col-md-6 mb-4">
								<img src="~/Finlab/images/big/img2.jpg" class="rounded" alt="" style="width:100%;">
							</a>
							<a href="~/Finlab/images/big/img3.jpg" data-src="../Finlab/images/big/img3.jpg" class=" lg-item col-lg-3 col-md-6 mb-4">
								<img src="~/Finlab/images/big/img3.jpg" class="rounded" alt="" style="width:100%;">
							</a>
							<a href="~/Finlab/images/big/img4.jpg" data-src="../Finlab/images/big/img4.jpg" class="lg-item col-lg-3 col-md-6 mb-4">
								<img src="~/Finlab/images/big/img4.jpg" class="rounded" alt="" style="width:100%;">
							</a>
							<a href="~/Finlab/images/big/img5.jpg" data-src="../Finlab/images/big/img5.jpg" class="lg-item col-lg-3 col-md-6 mb-4">
								<img src="~/Finlab/images/big/img5.jpg" class="rounded" alt="" style="width:100%;">
							</a>
							<a href="~/Finlab/images/big/img6.jpg" data-src="../Finlab/images/big/img6.jpg" class="lg-item col-lg-3 col-md-6 mb-4">
								<img src="~/Finlab/images/big/img6.jpg" class="rounded" alt="" style="width:100%;">
							</a>
							<a href="~/Finlab/images/big/img7.jpg" data-src="../Finlab/images/big/img7.jpg" class="lg-item col-lg-3 col-md-6 mb-4">
								<img src="~/Finlab/images/big/img7.jpg" class="rounded" alt="" style="width:100%;">
							</a>
							<a href="~/Finlab/images/big/img8.jpg" data-src="../Finlab/images/big/img8.jpg" class=" lg-item col-lg-3 col-md-6 mb-4">
								<img src="~/Finlab/images/big/img8.jpg" class="rounded" alt="" style="width:100%;">
							</a>
						</div>
					</div>
				</div>
				<!-- /# card -->
			</div>
		</div>

	</div>
</div>

@section Finlab_style
	{
	<!-- Light Gallery -->
	<link href="~/Finlab/vendor/lightgallery/dist/css/lightgallery.css" rel="stylesheet">
	<link href="~/Finlab/vendor/lightgallery/dist/css/lg-thumbnail.css" rel="stylesheet">
	<link href="~/Finlab/vendor/lightgallery/dist/css/lg-zoom.css" rel="stylesheet">
}
@section Finlab_script
	{
	<script src="~/Finlab/vendor/global/global.min.js"></script>
	<!-- Light Gallery -->
	<script src="~/Finlab/vendor/lightgallery/dist/lightgallery.min.js"></script>
	<script src="~/Finlab/vendor/lightgallery/dist/plugins/thumbnail/lg-thumbnail.min.js"></script>
	<script src="~/Finlab/vendor/lightgallery/dist/plugins/zoom/lg-zoom.min.js"></script>
	<script src="~/Finlab/vendor/bootstrap-select/dist/js/bootstrap-select.min.js"></script>
	<script src="~/Finlab/js/custom.min.js"></script>
	<script src="~/Finlab/js/dlabnav-init.js"></script>
	
	
}