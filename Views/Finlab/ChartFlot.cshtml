    <script src="~/Finlab/vendor/apexchart/apexchart.js"></script>
    <!-- Chart piety plugin files -->
    <script src="~/Finlab/vendor/peity/jquery.peity.min.js"></script>
    <script src="~/Finlab/vendor/flot/jquery.flot.js"></script>
    <script src="~/Finlab/vendor/flot/jquery.flot.pie.js"></script>
    <script src="~/Finlab/vendor/flot/jquery.flot.resize.js"></script>
