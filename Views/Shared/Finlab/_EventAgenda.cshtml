<div class="modal fade" id="exampleModal" tabindex="-1" aria-labelledby="exampleModalLabel" aria-hidden="true">
	<div class="modal-dialog">
		<div class="modal-content">
			<div class="modal-header">
				<h5 class="modal-title" id="exampleModalLabel">Event Agenda</h5>
				<button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
			</div>
			<div class="modal-body">
				<div class="mb-3 d-block">
					<label class="form-label d-block mb-2">Enter Name</label>
					<input type="text" class="form-control w-100 mb-3" placeholder="Name" aria-label="name">
				</div>
				<div class="mb-3 d-block">
					<label class="form-label d-block mb-2">Enter Position</label>
					<input type="text" class="form-control w-100" placeholder="Position" aria-label="position">
				</div>
			</div>
			<div class="modal-footer">
				<button type="button" class="btn btn-danger light" data-bs-dismiss="modal">Close</button>
				<button type="button" class="btn btn-primary">Save changes</button>
			</div>
		</div>
	</div>
</div>

